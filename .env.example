# Copy this file to .env.local and fill in your own values
# Demo mode keeps using mock data. Set provider to "finnhub" to enable the live API.
NEXT_PUBLIC_MARKET_DATA_PROVIDER=demo

# Required only when using the Finnhub provider
NEXT_PUBLIC_FINNHUB_API_KEY=

# Optional overrides (defaults shown)
NEXT_PUBLIC_MARKET_DATA_TIMEOUT_MS=10000
NEXT_PUBLIC_MARKET_DATA_MAX_RETRIES=2
NEXT_PUBLIC_MARKET_DATA_BACKOFF_MS=500
